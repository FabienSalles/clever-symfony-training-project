<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="app.repository.in_memory.artist.class">AppBundle\Repository\InMemory\ArtistRepository</parameter>
        <parameter key="app.repository.in_memory.track.class">AppBundle\Repository\InMemory\TrackRepository</parameter>
    </parameters>

    <services>
        <service id="app.repository.in_memory.artist" class="%app.repository.in_memory.artist.class%">
        </service>

        <service id="app.repository.in_memory.track" class="%app.repository.in_memory.track.class%">
        </service>

        <service id="app.form_type.artist" class="AppBundle\Form\Type\ArtistType">
            <tag name="form.type" />
        </service>
    </services>
</container>